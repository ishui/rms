		<Sql>
			<Name>Delete</Name>
			<String> Delete from Model  where  ModelCode=@ModelCode</String>
			<CommandType>Text</CommandType>
			<Param>
				<Name>@ModelCode</Name>
				<Column>ModelCode</Column>
				<SqlDbType>VarChar</SqlDbType>
			</Param>
		</Sql>
		<Sql>
			<Name>Select</Name>
			<String> select * from Model where  ModelCode=@ModelCode</String>
			<CommandType>Text</CommandType>
			<Param>
				<Name>@ModelCode</Name>
				<Column>ModelCode</Column>
				<SqlDbType>VarChar</SqlDbType>
			</Param>
		</Sql>
		<Sql>
			<Name>SelectAll</Name>
			<String> select a.* from Model a</String>
			<CommandType>Text</CommandType>
		</Sql>
		<Sql>
			<Name>SelectDoorNum</Name>
			<String> select a.*,(select count(*) from Room where ModelCode = a.ModelCode) as DoorNum
			   ,(select PictrueName from photos where pictureCode = a.ImageCode) as picName
			   , dbo.GetPBSTypeFullName(HouseType) as PBSTypeName
			     from Model a</String>
			<CommandType>Text</CommandType>
		</Sql>
		<Sql>
			<Name>SelectCountByBuildingCode</Name>
			<String> select a.*
			   ,(select count(*) from Room where BuildingCode = @BuildingCode and ModelCode = a.ModelCode) as RoomCount
			   ,(select sum(isnull(PreBuildArea, 0)) from Room where BuildingCode = @BuildingCode and ModelCode = a.ModelCode) as TotalPreBuildArea
			   ,(select sum(isnull(PreRoomArea, 0)) from Room where BuildingCode = @BuildingCode and ModelCode = a.ModelCode) as TotalPreRoomArea
			   ,(select sum(isnull(BuildArea, 0)) from Room where BuildingCode = @BuildingCode and ModelCode = a.ModelCode) as TotalBuildArea
			   ,(select sum(isnull(RoomArea, 0)) from Room where BuildingCode = @BuildingCode and ModelCode = a.ModelCode) as TotalRoomArea
			   ,(select PictrueName from photos where pictureCode = a.ImageCode) as picName
			     from Model a
			     where ProjectCode = @ProjectCode
			       and exists(select * from Room b where BuildingCode = @BuildingCode and ModelCode = a.ModelCode)
			     order by ModelName
			 </String>
			<CommandType>Text</CommandType>
			<Param>
				<Name>@ProjectCode</Name>
				<Column>ProjectCode</Column>
				<SqlDbType>VarChar</SqlDbType>
			</Param>
			<Param>
				<Name>@BuildingCode</Name>
				<Column>BuildingCode</Column>
				<SqlDbType>VarChar</SqlDbType>
			</Param>
		</Sql>
		<Sql>
			<Name>SelectNoUseByBuildingCode</Name>
			<String> select * from Model where ProjectCode = @ProjectCode and not exists (select * from V_BuildingModel where V_BuildingModel.ModelCode=Model.ModelCode and BuildingCode=@BuildingCode and BuildingModelCode != @BuildingModelCode )</String>
			<CommandType>Text</CommandType>
			<Param>
				<Name>@ProjectCode</Name>
				<Column>ProjectCode</Column>
				<SqlDbType>VarChar</SqlDbType>
			</Param>
			<Param>
				<Name>@BuildingCode</Name>
				<Column>BuildingCode</Column>
				<SqlDbType>VarChar</SqlDbType>
			</Param>
			<Param>
				<Name>@BuildingModelCode</Name>
				<Column>BuildingModelCode</Column>
				<SqlDbType>VarChar</SqlDbType>
			</Param>
		</Sql>
